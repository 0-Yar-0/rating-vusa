    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "pg": "^8.9.0",
    "zod": "^3.20.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "prisma": "^5.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.0.0",
    "jest": "^29.0.0",
    "supertest": "^6.3.0"
  }
